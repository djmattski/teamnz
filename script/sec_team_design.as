package script {		import flash.display.*;	import flash.events.*;	import flash.geom.*;	import flash.utils.*;	import com.greensock.*;	import com.greensock.easing.*;		public class sec_team_design extends Sprite {				public var theAppBase:appBase;				public function sec_team_design(mainApp) {			// constructor code			theAppBase = mainApp;						//Configure the placement			var nameArr:Array = new Array();			nameArr[0] = 'a_bird_profile';			nameArr[1] = 'a_corkery_profile';			nameArr[2] = 'a_greenwood_profile';			nameArr[3] = 'a_kensington_profile';			nameArr[4] = 'b_mclay_profile';			nameArr[5] = 'c_blake_profile';			nameArr[6] = 'c_laquette_profile';			nameArr[7] = 'c_miller_profile';			nameArr[8] = 'd_bernasconi_profile';			nameArr[9] = 'j_france_profile';			nameArr[10] = 'l_boismain_profile';			nameArr[11] = 'm_biviot_profile';			nameArr[12] = 'm_mcelwee_profile';			nameArr[13] = 'm_seo_profile';			nameArr[14] = 'n_hutchins_profile';			nameArr[15] = 'n_shaver_profile';			nameArr[16] = 'p_flett_profile';			nameArr[17] = 'p_melvin_profile';			nameArr[18] = 's_collie_profile';			nameArr[19] = 't_win_profile';			var posArr:Array = new Array();			var column:Number = -1;			var columnDist:int = 200;			var rowDist:int = 40;			var theRow:int = 0;			for(var i:int = 0; i < 20; i ++){				//4 x 5 grid				if(i % 5 == 0){					column ++;				}				var theX = column * columnDist;				var theY = theRow * rowDist;				theRow ++;				if(theRow > 4){					theRow = 0;				}				posArr[i] = new Array();				posArr[i][0] = theX;				posArr[i][1] = theY;				var nameBit:String = tools.getNameBit(nameArr[i]);				var classRef:Class = getDefinitionByName(nameBit+'_main_profile_button') as Class;				var newPerson:MovieClip = new classRef();				newPerson.name = nameArr[i];				newPerson.x = posArr[i][0];				newPerson.y = posArr[i][1];				addChild(newPerson);			}					}				/**		* Public functions		*/			}	}