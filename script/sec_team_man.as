package script {		import flash.display.*;	import flash.events.*;	import flash.geom.*;	import flash.utils.*;	import com.greensock.*;	import com.greensock.easing.*;		public class sec_team_man extends Sprite {				public var theAppBase:appBase;				public function sec_team_man(mainApp) {			// constructor code			theAppBase = mainApp;						//Configure the placement			var nameArr:Array = new Array();			nameArr[0] = 'g_paykel_profile';			nameArr[1] = 'j_farmer_profile';			nameArr[2] = 'k_shoebridge_profile';			nameArr[3] = 'm_ora_profile';			var posArr:Array = new Array();			var column:Number = -1;			var columnDist:int = 205;			var rowDist:int = 42;			var theRow:int = 0;			for(var i:int = 0; i < 4; i ++){				//5 x 5 grid				if(i % 5 == 0){					column ++;				}				var theX = column * columnDist;				var theY = theRow * rowDist;				theRow ++;				if(theRow > 4){					theRow = 0;				}				posArr[i] = new Array();				posArr[i][0] = theX;				posArr[i][1] = theY;				var nameBit:String = tools.getNameBit(nameArr[i]);				var classRef:Class = getDefinitionByName(nameBit+'_main_profile_button') as Class;				var newPerson:MovieClip = new classRef();				newPerson.name = nameArr[i];				newPerson.x = posArr[i][0];				newPerson.y = posArr[i][1];				addChild(newPerson);			}		}	}	}