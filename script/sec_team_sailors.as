package script {		import flash.display.*;	import flash.events.*;	import flash.geom.*;	import flash.utils.*;	import com.greensock.*;	import com.greensock.easing.*;		public class sec_team_sailors extends Sprite {				public var theAppBase:appBase;				public function sec_team_sailors(mainApp) {			// constructor code			theAppBase = mainApp;						//Configure the placement			var nameArr:Array = new Array();			nameArr[0] = 'a_beashel_profile';			nameArr[1] = 'c_mcasey_profile';			nameArr[2] = 'c_ward_profile';			nameArr[3] = 'd_barker_profile';			nameArr[4] = 'd_saward_profile';			nameArr[5] = 'g_ashby_profile';			nameArr[6] = 'g_dalton_profile';			nameArr[7] = 'g_loretz_profile';			nameArr[8] = 'j_dagg_profile';			nameArr[9] = 'j_lomas_profile';			nameArr[10] = 'r_davies_profile';			nameArr[11] = 'r_meacham_profile';			nameArr[12] = 'r_waddell_profile';			nameArr[13] = 't_rae_profile';			nameArr[14] = 'w_macfalane_profile';			var posArr:Array = new Array();			var column:Number = -1;			var columnDist:int = 200;			var rowDist:int = 40;			var theRow:int = 0;			for(var i:int = 0; i < 15; i ++){				//4 x 5 grid				if(i % 5 == 0){					column ++;				}				var theX = column * columnDist;				var theY = theRow * rowDist;				theRow ++;				if(theRow > 4){					theRow = 0;				}				posArr[i] = new Array();				posArr[i][0] = theX;				posArr[i][1] = theY;				var nameBit:String = tools.getNameBit(nameArr[i]);				var classRef:Class = getDefinitionByName(nameBit+'_main_profile_button') as Class;				var newPerson:MovieClip = new classRef();				newPerson.name = nameArr[i];				newPerson.x = posArr[i][0];				newPerson.y = posArr[i][1];				newPerson.buttonMode = true;				newPerson.addEventListener(MouseEvent.ROLL_OVER, appBase.rollOn);				newPerson.addEventListener(MouseEvent.ROLL_OUT, appBase.rollOff);				newPerson.addEventListener(MouseEvent.MOUSE_DOWN, theAppBase.newSub);				addChild(newPerson);			}					}				/**		* Public functions		*/			}	}