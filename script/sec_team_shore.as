package script {		import flash.display.*;	import flash.events.*;	import flash.geom.*;	import flash.utils.*;	import com.greensock.*;	import com.greensock.easing.*;		public class sec_team_shore extends Sprite {				public var theAppBase:appBase;		private var backArr:global_back_arr;		private var backArrCont:Sprite;		private var currPos:int;		private var holder1:Sprite;		private var holder2:Sprite;				public function sec_team_shore(mainApp) {			// constructor code			theAppBase = mainApp;						//Configure the placement			var nameArr:Array = new Array();			nameArr[0] = 'a_gray_profile';			nameArr[1] = 'a_higby_profile';			nameArr[2] = 'b_heathwaite_profile';			nameArr[3] = 'b_malcolm_profile';			nameArr[4] = 'b_moss_profile';			nameArr[5] = 'c_dickie_profile';			nameArr[6] = 'c_holland_profile';			nameArr[7] = 'c_holmes_profile';			nameArr[8] = 'c_nickalls_profile';			nameArr[9] = 'c_salthouse_profile';			nameArr[10] = 'd_brooks_profile';			nameArr[11] = 'd_ewing_profile';			nameArr[12] = 'd_french_profile';			nameArr[13] = 'd_karaitiana_profile';			nameArr[14] = 'd_mclean_profile';			nameArr[15] = 'e_bray_profile';			nameArr[16] = 'e_thorpe_profile';			nameArr[17] = 'g_endean_profile';			nameArr[18] = 'j_armstrong_profile';			nameArr[19] = 'j_douglas_profile';			nameArr[20] = 'j_graham_profile';			nameArr[21] = 'j_taylor_profile';			nameArr[22] = 'l_manson_profile';			nameArr[23] = 'l_rosewarne_profile';			nameArr[24] = 'm_gwilliam_profile';			nameArr[25] = 'm_hibbard_profile';			nameArr[26] = 'n_burridge_profile';			nameArr[27] = 'o_breton_profile';			nameArr[28] = 'r_barry_profile';			nameArr[29] = 'r_kiff_profile';			nameArr[30] = 's_regan_profile';			nameArr[31] = 't_lamond_profile';			nameArr[32] = 'v_vattuone_profile';			var posArr:Array = new Array();			var column:Number = -1;			var columnDist:int = 200;			var rowDist:int = 40;			var theRow:int = 0;			var theX:int = 0;			var theY:int = 0;			//Create two holders			holder1 = new Sprite();			addChild(holder1);			holder2 = new Sprite();			holder2.x = 1300;			addChild(holder2);						//First loop			for(var i:int = 0; i < 25; i ++){				//4 x 5 grid				if(i % 5 == 0){					column ++;				}				theX = column * columnDist;				theY = theRow * rowDist;				theRow ++;				if(theRow > 4){					theRow = 0;				}				posArr[i] = new Array();				posArr[i][0] = theX;				posArr[i][1] = theY;				var nameBit1:String = tools.getNameBit(nameArr[i]);				var classRef1:Class = getDefinitionByName(nameBit1+'_main_profile_button') as Class;				var newPerson1:MovieClip = new classRef1();				newPerson1.name = nameArr[i];				newPerson1.x = posArr[i][0];				newPerson1.y = posArr[i][1];				holder1.addChild(newPerson1);			}			//Second loop - reset			theRow = 0;			column = -1;			for(var j:int = 25; j < 33; j ++){				//4 x 5 grid				if(j % 5 == 0){					column ++;				}				theX = column * columnDist;				theY = theRow * rowDist;				theRow ++;				if(theRow > 4){					theRow = 0;				}				posArr[j] = new Array();				posArr[j][0] = theX;				posArr[j][1] = theY;				var nameBit2:String = tools.getNameBit(nameArr[j]);				var classRef2:Class = getDefinitionByName(nameBit2+'_main_profile_button') as Class;				var newPerson2:MovieClip = new classRef2();				newPerson2.name = nameArr[j];				newPerson2.x = posArr[j][0];				newPerson2.y = posArr[j][1];				holder2.addChild(newPerson2);			}						//Navigate between			backArr = new global_back_arr();			backArr.name = 'backArr';			backArr.x = 20 + (backArr.width/2);			backArr.y = 20;			backArr.rotation = 180;			backArrCont = new Sprite();			backArrCont.name = 'backArrCont';			backArrCont.x = 1024 - 70;			backArrCont.y = 50;			backArrCont.buttonMode = true;			addChild(backArrCont);			giveMeABackBtn('profilesBack',0xD4A630,0x8E6F32);			currPos = 1;		}				/**		* Public functions		*/		public function giveMeABackBtn(theName:String, theColour1:uint, theColour2:uint):void{			var thisBack:Sprite = new Sprite();			thisBack.name = theName;			thisBack.graphics.beginFill(theColour1, 1);			thisBack.graphics.drawRect(0, 0, 40, 40);			thisBack.graphics.endFill();			thisBack.width = 40;			thisBack.height = 40;			backArrCont.addChild(thisBack);			backArrCont.addChild(backArr);			backArrCont.addEventListener(MouseEvent.ROLL_OVER, function(){				//alter colour 				var my_color:ColorTransform = new ColorTransform();				my_color.color = theColour2;				thisBack.transform.colorTransform = my_color;			});			backArrCont.addEventListener(MouseEvent.ROLL_OUT, function(){				//alter colour				var my_color:ColorTransform = new ColorTransform();				my_color.color = theColour1;				thisBack.transform.colorTransform = my_color;			});			backArrCont.addEventListener(MouseEvent.MOUSE_DOWN, goBack);		}				public function goBack(e:MouseEvent):void{			if(currPos == 1){				TweenLite.to(holder1,0.5,{x:-1300, ease:Sine.easeIn, onComplete:function(){					backArr.rotation = 0;					backArr.x = backArr.x - 20;					currPos = 2;				}});				TweenLite.to(holder2,0.5,{x:0, ease:Sine.easeOut});			}else{				TweenLite.to(holder2,0.5,{x:1300, ease:Sine.easeIn});				TweenLite.to(holder1,0.5,{x:0, ease:Sine.easeOut, onComplete:function(){					backArr.rotation = 180;					backArr.x = backArr.x + 20;					currPos = 1;				}});			}		}	}	}