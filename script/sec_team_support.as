package script {		import flash.display.*;	import flash.events.*;	import flash.geom.*;	import flash.utils.*;	import com.greensock.*;	import com.greensock.easing.*;	import script.ui.BaseButton;		public class sec_team_support extends Sprite {				public var theAppBase:appBase;				public function sec_team_support(mainApp) {			// constructor code			theAppBase = mainApp;						//Configure the placement			var nameArr:Array = new Array();			nameArr[0] = 'a_nottage_profile';			nameArr[1] = 'c_cameron_profile';			nameArr[2] = 'c_hicks_profile';			nameArr[3] = 'c_stokes_profile';			nameArr[4] = 'd_mason_profile';			nameArr[5] = 'd_reid_profile';			nameArr[6] = 'd_slyfield_profile';			nameArr[7] = 'g_kemp_profile';			nameArr[8] = 'h_hooper_profile';			nameArr[9] = 'h_lynskey_profile';			nameArr[10] = 'i_stewart_profile';			nameArr[11] = 'j_allen_profile';			nameArr[12] = 'j_lynskey_profile';			nameArr[13] = 'l_edser_profile';			nameArr[14] = 'm_yates_profile';			nameArr[15] = 'p_wilson_profile';			nameArr[16] = 'r_badham_profile';			nameArr[17] = 'r_blackman_profile';			nameArr[18] = 'r_davis_profile';			nameArr[19] = 'r_green_profile';			nameArr[20] = 's_stubbs_profile';			nameArr[21] = 'w_douglas_profile';			var posArr:Array = new Array();			var column:Number = -1;			var columnDist:int = 200;			var rowDist:int = 40;			var theRow:int = 0;			for(var i:int = 0; i < 22; i ++){				//4 x 5 grid				if(i % 5 == 0){					column ++;				}				var theX = column * columnDist;				var theY = theRow * rowDist;				theRow ++;				if(theRow > 4){					theRow = 0;				}				posArr[i] = new Array();				posArr[i][0] = theX;				posArr[i][1] = theY;				var nameBit:String = tools.getNameBit(nameArr[i]);								//Exceptions to two items				if(i == 6 || i == 9){					var classRef:Class = getDefinitionByName(nameBit+'_main_profile_button') as Class;					var newPerson = new BaseButton(new classRef());					newPerson.content.name = nameArr[i];					newPerson.content.x = posArr[i][0];					newPerson.content.y = posArr[i][1];					newPerson.content.addEventListener(MouseEvent.MOUSE_DOWN, theAppBase.newSub);					newPerson.enable();										addChild(newPerson.content);				}else{					var classRef1:Class = getDefinitionByName(nameBit+'_main_profile_button') as Class;					var newPerson1:MovieClip = new classRef1();					newPerson1.name = nameArr[i];					newPerson1.x = posArr[i][0];					newPerson1.y = posArr[i][1];										addChild(newPerson1);				}											}		}	}	}